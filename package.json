		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"jspdf": "^2.5.1",
		"svelte-i18n": "^4.0.0"
	},
	"pre-commit": [
		"sc"
